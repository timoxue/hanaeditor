    "css-loader": "^0.28.7",
    "electron": "^1.7.10",
    "electron-packager": "^10.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "is-electron-renderer": "^2.0.1",
    "style-loader": "^0.19.1",
    "watchify": "^3.9.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
